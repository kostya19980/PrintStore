    <partial name="_CookieConsentPartial" />
    <div class="account_modal_background">
        <div class="account_modal_window">
            <div class="account_modal_header">
                <button class="modal_header_button" style="border-top-left-radius:8px;" value="login">
                    Вход
                </button>
                <button class="modal_header_button" value="register">
                    Регистрация
                </button>
                <button class="modal_header_close">
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M18 6L6 18M6 6L18 18" stroke="#ABACB3" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                    </svg>
                </button>
            </div>
            <div id="login_block" class="auth_block">
                @{
                    var LoginModel = new PrintStore.ViewModel.LoginModel();
                    await Html.RenderPartialAsync("~/Views/Account/_Login.cshtml", LoginModel);
                }
            </div>
            <div id="register_block" class="auth_block">
                @{
                    var RegisterModel = new PrintStore.ViewModel.RegisterModel();
